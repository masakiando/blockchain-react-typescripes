{
  "name": "typescript-node-react-blockchain",
  "version": "0.1.0",
  "main": "dist/index.js",
  "author": "Your Name <you@example.com>",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "dev:watch": "ts-node-dev --respawn src/index.ts",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "start": "node .",
    "lint": "eslint --ext .ts,.js src/",
    "format": "prettier --write 'src/**/*.{ts,js}'",
    "precommit": "lint-staged",
    "test": "mocha  -r ts-node/register \"src/**/*.spec.ts\" \"test\""
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint --ext .ts,.js",
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": "12.7.0",
    "npm": "6.12.0"
  },
  "keywords": [],
  "devDependencies": {
    "@types/chai": "4.2.8",
    "@types/mocha": "7.0.1",
    "@types/node": "12.7.11",
    "@typescript-eslint/eslint-plugin": "2.3.3",
    "@typescript-eslint/parser": "2.3.3",
    "chai": "4.2.0",
    "eslint": "6.5.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.0.8",
    "lint-staged": "9.4.2",
    "mocha": "7.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "ts-node": "8.6.2",
    "ts-node-dev": "1.0.0-pre.43",
    "typescript": "3.6.3"
  },
  "dependencies": {
    "crypto": "1.0.1"
  }
}
